{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "arborstone/slide",
    "title": "Arborstone University Slide",
    "supports": {
        "align": ["full"]
    },
    "attributes": {
        "themeimage": { "type": "string" },
        "align": { "type": "string", "default": "full" },
        "imgID": { "type": "number" },
        "imgURL": { "type": "string" }
    },
    "editorScript": "file:./index.js",
    "render": "file:./render.php"
}